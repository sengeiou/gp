<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:id="@+id/ubt_layout_wifi_lsit_item"
    android:layout_height="@dimen/ubt_edt_height"
    android:background="@null">

    <TextView
        style="@style/UBTTipsTxtTheme"
        android:id="@+id/ubt_tv_wifi_name"
        android:layout_width="match_parent"
        android:layout_height="@dimen/ubt_edt_height"
        android:layout_centerVertical="true"
        android:gravity="center_vertical|left"
        android:textSize="15sp"
        android:lines="1"
        android:layout_toStartOf="@+id/ubt_img_wifi_singal"
        android:text="TextView" />
    <ImageView
        android:id="@+id/ubt_img_wifi_singal"
        android:layout_width="@dimen/ubt_wifi_list_singal_width"
        android:layout_height="@dimen/ubt_wifi_list_singal_height"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        app:srcCompat="@drawable/ic_wifi_free_full" />
    <View
        android:id="@+id/ubt_wifi_list_divider"
        android:layout_alignParentBottom="true"
        android:layout_width="match_parent"
        android:background="@color/ubt_wifi_list_divider"
        android:layout_height="@dimen/ubt_1px"/>
</RelativeLayout>