<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--UbtImageButton 自定义属性-->
    <declare-styleable name="UbtImageButtonStyle">
        <attr name="icon" format="reference"/>
        <attr name="iconWidth" format="dimension"/>
        <attr name="iconHeight" format="dimension"/>
        <attr name="iconMargin" format="dimension"/>
        <attr name="iconMarginTop" format="dimension"/>
        <attr name="iconMarginLeft" format="dimension"/>
        <attr name="iconMarginRight" format="dimension"/>
        <attr name="iconMarginBottom" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="UbtIconEditTextStyle">
        <attr name="defaultIcon" format="reference"/>
    </declare-styleable>
    <declare-styleable name="UbtPsdEditTextStyle">
        <attr name="psdIcon" format="reference"/>
    </declare-styleable>
    <declare-styleable name="UbtRightTxtButtonStyle">
        <attr name="rightText" format="reference|string"/>
        <attr name="rightTextColor" format="reference|color"/>
        <attr name="rightTextPadding" format="reference|dimension"/>
        <attr name="rightTextSize" format="dimension|reference"/>
    </declare-styleable>
</resources>